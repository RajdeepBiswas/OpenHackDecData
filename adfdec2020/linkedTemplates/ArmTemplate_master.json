{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdec2020"
		},
		"AzureDataLakeStorageFourthCoffee_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageFourthCoffee'"
		},
		"AzureSqlDatabaseCloudSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseCloudSales'"
		},
		"AzureSqlDatabaseCloudStreaming_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseCloudStreaming'"
		},
		"SouthridgeAzureDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SouthridgeAzureDataLake'"
		},
		"moviesCosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'moviesCosmosDb'"
		},
		"AzureDataLakeStorageFourthCoffee_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://openhackdec2020adls.dfs.core.windows.net"
		},
		"SouthridgeAzureDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storagedec.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdec2020_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageFourthCoffee_accountKey": {
						"value": "[parameters('AzureDataLakeStorageFourthCoffee_accountKey')]"
					},
					"AzureSqlDatabaseCloudSales_connectionString": {
						"value": "[parameters('AzureSqlDatabaseCloudSales_connectionString')]"
					},
					"AzureSqlDatabaseCloudStreaming_connectionString": {
						"value": "[parameters('AzureSqlDatabaseCloudStreaming_connectionString')]"
					},
					"SouthridgeAzureDataLake_accountKey": {
						"value": "[parameters('SouthridgeAzureDataLake_accountKey')]"
					},
					"moviesCosmosDb_connectionString": {
						"value": "[parameters('moviesCosmosDb_connectionString')]"
					},
					"AzureDataLakeStorageFourthCoffee_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageFourthCoffee_properties_typeProperties_url')]"
					},
					"SouthridgeAzureDataLake_properties_typeProperties_url": {
						"value": "[parameters('SouthridgeAzureDataLake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfdec2020_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfdec2020_ArmTemplate_0')]"
			]
		}
	]
}